<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login_page">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:elevation="20dp"
        android:text="in"
        android:textColor="#FF0000"
        android:textSize="40sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:elevation="20dp"
        android:text="Tea Delight"
        android:textColor="@color/black"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:elevation="20dp"
        android:fontFamily="@font/roboto_thin_italic"
        android:gravity="center"
        android:text="INTI Cafeteria Mobile Application"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="223dp" />

    <Button
        android:id="@+id/buttonHomePage"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button_shape"
        android:elevation="20dp"
        android:text="Login"
        android:textColor="@color/white"
        app:backgroundTint="@color/button_background_color"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="589dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="347dp"
        android:layout_height="577dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/box"
        android:backgroundTint="#FFFFFF"
        android:breakStrategy="high_quality"
        android:drawingCacheQuality="auto"
        android:elevation="10dp"
        android:gravity="center_horizontal"
        android:paddingTop="20dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="25sp"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/shapeThree3"
        android:layout_width="88dp"
        android:layout_height="305dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shape3" />

    <ImageView
        android:id="@+id/shapeSeven2"
        android:layout_width="130dp"
        android:layout_height="319dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/shape7" />

    <ImageView
        android:id="@+id/shapeSix2"
        android:layout_width="118dp"
        android:layout_height="273dp"
        app:srcCompat="@drawable/shape6"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="429dp" />

    <ImageView
        android:id="@+id/shapeOne3"
        android:layout_width="199dp"
        android:layout_height="170dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shape1" />

    <Button
        android:id="@+id/buttonBackLanding"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="96dp"
        android:layout_height="39dp"
        android:background="@drawable/button_shape"
        android:elevation="20dp"
        android:text="< Back"
        android:textColor="@color/black"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="109dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="56dp"
        android:layout_height="48dp"
        android:elevation="20dp"
        android:ems="10"
        android:text="Login"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="263dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="149dp"
        android:layout_height="24dp"
        android:elevation="20dp"
        android:text="Password"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="420dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="149dp"
        android:layout_height="24dp"
        android:elevation="20dp"
        android:text="Email"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="332dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="301dp"
        android:layout_height="41dp"
        android:elevation="20dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="444dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="302dp"
        android:layout_height="42dp"
        android:elevation="20dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="356dp" />

    <Button
        android:id="@+id/buttonForgetPassword"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="299dp"
        android:layout_height="36dp"
        android:background="@drawable/button_shape"
        android:elevation="20dp"
        android:text="Forget Password?"
        android:textColor="#03A9F4"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="504dp" />


</androidx.constraintlayout.widget.ConstraintLayout>