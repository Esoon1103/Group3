<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LandingPage">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginBottom="90dp"
        android:background="@drawable/box"
        android:backgroundTint="#FFFFFF"
        android:breakStrategy="high_quality"
        android:drawingCacheQuality="auto"
        android:elevation="2dp"
        android:gravity="center_horizontal"
        android:paddingTop="20dp"
        android:text="I am a..."
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="25sp"
        android:textStyle="italic"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:visibility="visible" />

    <Button
        android:id="@+id/buttonLoginLanding"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button_shape"
        android:elevation="65dp"
        android:text="Login"
        android:textColor="@color/white"
        app:backgroundTint="@color/button_background_color"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.41" />


    <Button
        android:id="@+id/buttonSignUpLanding"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button_shape"
        android:text="Sign Up"
        android:textColor="#880808"
        app:backgroundTint="#FFFFFF"

        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.792" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginBottom="52dp"
        android:fontFamily="@font/roboto_thin_italic"
        android:gravity="center"
        android:text="INTI Cafeteria Mobile Application"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="15sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="90dp"
        android:text="Tea Delight"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginStart="85dp"
        android:text="in"
        android:textColor="#FF0000"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/shapeSix"
        android:layout_width="100dp"
        android:layout_height="242dp"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/shape6" />

    <ImageView
        android:id="@+id/shapeThree"
        android:layout_width="70dp"
        android:layout_height="261dp"
        android:layout_marginTop="54dp"
        app:layout_constraintBottom_toTopOf="@+id/tin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/shapeTwo"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/shape3" />

    <ImageView
        android:id="@+id/shapeFive"
        android:layout_width="215dp"
        android:layout_height="94dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/egg"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/shape5" />

    <ImageView
        android:id="@+id/shapeTwo"
        android:layout_width="169dp"
        android:layout_height="87dp"
        android:layout_marginStart="21dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/shapeOne"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shape2" />

    <ImageView
        android:id="@+id/egg"
        android:layout_width="89dp"
        android:layout_height="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/shapeSeven"
        app:srcCompat="@drawable/foodemoji3" />

    <ImageView
        android:id="@+id/cake"
        android:layout_width="69dp"
        android:layout_height="91dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/shapeSix"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/foodemoji4" />

    <ImageView
        android:id="@+id/shapeOne"
        android:layout_width="117dp"
        android:layout_height="99dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shape1" />

    <ImageView
        android:id="@+id/sandwich"
        android:layout_width="112dp"
        android:layout_height="93dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/shapeOne"
        app:layout_constraintTop_toTopOf="@+id/shapeOne"
        app:srcCompat="@drawable/foodemoji2" />

    <ImageView
        android:id="@+id/tin"
        android:layout_width="82dp"
        android:layout_height="140dp"
        android:layout_marginTop="97dp"
        app:layout_constraintBottom_toTopOf="@+id/shapeSeven"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/foodemoji1" />

    <ImageView
        android:id="@+id/shapeSeven"
        android:layout_width="85dp"
        android:layout_height="205dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/shape7" />


</androidx.constraintlayout.widget.ConstraintLayout>